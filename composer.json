{
  "name": "italia/spid-symfony-bundle",
  "description": "A Bundle to make Symfony3 capable of using SPID authentication, based on italia/spid-php-lib",
  "type": "bundle",
  "keywords": [ "oauth","ws-federation","sp","idp", "spid" ],
  "homepage": "https://github.com/italia/spid-symfony-bundle",
  "license": "BSD-3-Clause",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Marco Albarelli",
      "email": "info@marcoalbarelli.eu"
    }
  ],
  "require": {
    "php": ">=7.4",
    "symfony/framework-bundle": "^5.0",
    "symfony/console": "^5.0",
    "symfony/process": "^5.0",
    "symfony/yaml": "^5.0",
    "twig/twig": "^3.0",
    "italia/spid-php-lib": "^0.35"
  },
  "require-dev": {
    "endouble/symfony3-custom-coding-standard": "^2.17",
    "symfony/doctrine-bridge": "^5.0",
    "symfony/filesystem": "^5.0",
    "symfony/phpunit-bridge": "^5.0",
    "doctrine/orm": "^2.7",
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "Italia\\SpidSymfonyBundle\\": ""
    }
  },
  "conflict": {
      "symfony/symfony": "*"
  },
  "extra": {
      "symfony": {
          "allow-contrib": true,
          "require": "5.0.*"
      }
  }
}
